<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<!DOCTYPE properties SYSTEM "http://java.sun.com/dtd/properties.dtd">
<properties>
    <!-- ОБЯЗАТЕЛЬНЫЕ ПАРАМЕТРЫ -->
    <!-- Адрес доски в jira -->
    <entry key="board_url">https://jira.example.com/secure/RapidBoard.jspa?rapidView=00000</entry>

    <!-- Имя пользователя -->
    <entry key="user_name">username</entry>
    
    <!-- ОПЦИОНАЛЬНЫЕ ПАРАМЕТРЫ -->
    <!-- Дополнительный фильтр для отсева "лишних" issues -->
<!--    <entry key="jql_sub_filter">created >= '2022/01/01'</entry>-->
    <entry key="jql_sub_filter">created >= -12w</entry>
    
    <!-- Файл для вывода issues. по-умолчанию output.csv -->
    <entry key="output_file_name">output.csv</entry>
    
    <!-- Пароль от username. по-умолчанию не задан его можно указат в приложении -->
    <entry key="password"></entry>
</properties>